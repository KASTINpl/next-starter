{
  "name": "next-starter",
  "description": "Next.js starter project with typescript, emotion, immer, hooks, eslint, prettier, jest, cypress and storybook",
  "version": "0.0.1",
  "author": "Alper Ortac",
  "license": "MIT",
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "yarn run test:unit && yarn run test:e2e",
    "test:unit": "jest",
    "test:unit:coverage": "jest --coverage",
    "test:unit:update": "jest --updateSnapshot",
    "test:unit:ui": "majestic",
    "test:e2e": "cypress run",
    "test:e2e:ui": "cypress open",
    "lint:staged": "lint-staged",
    "typecheck": "tsc",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook"
  },
  "dependencies": {
    "@emotion/core": "^10.0.10",
    "@emotion/styled": "^10.0.10",
    "@zeit/next-typescript": "^1.1.1",
    "immer": "^3.1.1",
    "next": "^8.0.4",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "use-immer": "^0.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@storybook/addon-a11y": "^5.0.10",
    "@storybook/addon-actions": "^5.0.10",
    "@storybook/addon-knobs": "^5.0.10",
    "@storybook/addon-viewport": "^5.0.10",
    "@storybook/react": "^5.0.10",
    "@types/jest": "^24.0.11",
    "@types/next": "^8.0.3",
    "@types/node": "^11.13.6",
    "@types/react": "^16.8.14",
    "@types/react-dom": "^16.8.4",
    "@types/storybook__addon-actions": "^3.4.2",
    "@types/storybook__addon-knobs": "^5.0.0",
    "@types/storybook__react": "^4.0.1",
    "@typescript-eslint/eslint-plugin": "^1.6.1-alpha.14",
    "@typescript-eslint/parser": "^1.6.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-loader": "^8.0.5",
    "cypress": "^3.2.0",
    "cypress-testing-library": "^3.0.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-cypress": "^2.2.1",
    "eslint-plugin-flowtype": "^3.6.1",
    "eslint-plugin-import": "^2.17.1",
    "eslint-plugin-jest": "^22.4.1",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "husky": "^2.2.0",
    "jest": "^24.7.1",
    "lint-staged": "^8.1.6",
    "majestic": "^1.4.1",
    "prettier": "^1.17.0",
    "react-testing-library": "^6.1.2",
    "ts-jest": "^24.0.2",
    "typescript": "^3.4.4"
  }
}
